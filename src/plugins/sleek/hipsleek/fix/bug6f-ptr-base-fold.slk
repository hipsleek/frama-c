data arr_int {
  int val;
  arr_int next;
}.

pred arr_seg<p,n> == self=p & n=0
  or self::arr_int<_,q>*q::arr_seg<p,n-1>
  inv n>=0.

pred arr_seg2<p,n> == self=p & n=0
  or self::arr_int<5,q>*q::arr_seg2<p,n-1>
  inv n>=0.

 checkentail 
  a::arr_seg<p,n> 
   & 10<=i' 
   & i<=10 & 0<=i & n+i=5+10 & i'=i 
  |-  a::arr_seg2<q,10-i> 
      //* q::arr_seg<p,6> 
  .
 print residue.

 checkentail 
  a::arr_seg<p,n> 
   & 10<=i' 
   & i<=10 & 0<=i & n+i=5+10 & i'=i 
  |-  a::arr_seg2<q,10-i> 
      * q::arr_seg<p,nn>
  .

 checkentail 
  a::arr_seg<p,n> 
   & 10<=i' 
   & i<=10 & 0<=i & n+i=5+10 & i'=i & a=q
  |-   q::arr_seg<p,nn>
  .


 
 
