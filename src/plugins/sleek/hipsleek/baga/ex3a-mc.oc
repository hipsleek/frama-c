R:={[n,r]: 
     ((n>=101 & r+10=n) || (n<=100 & r=91))
  };

R;

M:= {[n,r]:n>100 & r=n-10
     || exists(n1,r1: n1=n+11 &
          # M(n1,r1)
       ((n1>=101 & r1+10=n1) || (n1<=100 & r1=91))
          # M(r1,r)
       && ((r1>=101 & r+10=r1) || (r1<=100 & r=91))
       & n<=100
     )};

M subset R;
R subset M;