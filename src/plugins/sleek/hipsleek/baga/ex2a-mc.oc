R:={[n,r]: 
   ((r>=91 & r+10=n) || (r>=91 & r+8>=n & 92>=r))
  };

R;

M:= {[n,r]:n>100 & r=n-10
     || exists(n1,r1: n1=n+12 &
          # M(n1,r1)
       ((r1>=91 & r1+10=n1) || (r1>=91 & r1+8>=n1 & 92>=r1))
          # M(r1,r)
       && ((r>=91 & r+10=r1) || (r>=91 & r+8>=r1 & 92>=r)) & n<=100
     )};

M subset R;
R subset M;