
data GTyp {
  GTyp f0;
  GTyp f1;
}.

pred PeList<y:GTyp> ==
 self = y
or (exists xp: self::GTyp<f0 = xp,f1 = _> * xp::PeList<y> & null != self).

/*
lemma_unsafe  self::PeList<y> * y::GTyp<f0 = z,f1 = _> -> self::PeList<z>.
*/

checkentail_exact x::PeList<y> * y::GTyp<f0 = z,f1 = _>
         |- x::PeList<z>.

expect Valid.