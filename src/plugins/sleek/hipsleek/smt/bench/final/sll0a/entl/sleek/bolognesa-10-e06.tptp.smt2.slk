
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail_exact x7::Sll_t<next = x1> * x4::ls<x9> * x9::ls<x2> * x10::Sll_t<next = x9> * x6::Sll_t<next = x2> * x2::ls<x8> * x1::Sll_t<next = x5> * x5::Sll_t<next = x8> * x3::Sll_t<next = x6> * x8::Sll_t<next = x3> * emp & null = null
         |- x10::ls<x9> * x7::ls<x8> * x4::ls<x2> * x2::ls<x8> * x8::ls<x2> * emp.
expect Fail.
