
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail_exact x2::ls<x3> * x1::Sll_t<next = x2> * x3::Sll_t<next = x1> * emp & null = null & null != x1 & null != x2 & null != x3 & x1 != x2 & x1 != x3
         |- x4::ls<x3> * x3::Sll_t<next = x4> * emp & x4 != x3.
expect Fail.
