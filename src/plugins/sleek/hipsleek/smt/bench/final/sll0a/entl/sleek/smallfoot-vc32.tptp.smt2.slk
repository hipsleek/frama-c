
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail_exact x1::ls<x4> * x4::Sll_t<next = x1> * emp & null = null & null != x1 & null != x2 & null != x3 & null != x4 & x2 != x3 & x2 != x4
         |- x5::ls<x4> * x4::Sll_t<next = x5> * emp.
expect Fail.
