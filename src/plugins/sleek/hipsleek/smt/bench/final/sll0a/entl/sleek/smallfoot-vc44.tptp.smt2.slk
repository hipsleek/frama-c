
data Sll_t {
  Sll_t next;
}.

pred ls<out:Sll_t> ==
 self = out
or (exists u: self::Sll_t<next = u> * u::ls<out> & self != out).

checkentail_exact x3::ls<null> * x4::ls<null> * x1::Sll_t<next = x4> * x2::Sll_t<next = x3> * emp & null = null & null != x1 & null != x2 & x3 != x1 & x3 != x2 & x1 != x2 & x1 != x4 & x2 != x4
         |- x2::ls<null> * x1::ls<null> * emp.
expect Valid.
