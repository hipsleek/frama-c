
data GTyp {
  GTyp f0;
  GTyp f1;
}.

pred P<> ==
self::one<> * self::Q<>.

pred Q<> ==
 self::zero<>
or (exists x1: x1::succ1rec<self> * x1::Q<>).

pred succ1rec<y1:GTyp> ==
self::zero<> * y1::one<>.

pred zero<> ==
null = self.

pred one<> ==
null != self.

checkentail_exact x0::P<>
         |- false.

expect Fail.

