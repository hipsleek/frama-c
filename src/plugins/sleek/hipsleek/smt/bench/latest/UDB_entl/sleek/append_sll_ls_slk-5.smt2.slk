
data node {
  node next;
}.

pred ll<> ==
 self = null
or (exists q_20: self::node<next = q_20> * q_20::ll<>).

pred lseg<p:node> ==
 self = p
or (exists p_19,q_18: self::node<next = q_18> * q_18::lseg<p_19> & p_19 = p).

pred ll_e1<> ==
(exists q: self::node<next = q> * q::ll<>).

pred ll_e2<> ==
(exists p,q: self::node<next = p> * q::ll<> & p = q).

pred node_e1<q:node> ==
(exists p: self::node<next = p> & p = q).

pred lseg_e1<p:node> ==
(exists q: self::lseg<p> & p = q).

checkentail_exact q::ll<> * xprm::node<next = q> & v1prm = q & q != null & xprm = x & yprm = y & x != null
         |- v1prm::ll<> * xprm::node<next = q> & v1prm = q & q != null & xprm = x & yprm = y & x != null & v1prm != null.

